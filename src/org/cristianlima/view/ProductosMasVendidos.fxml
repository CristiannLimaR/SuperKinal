            <Image url="@../image/Grafica1VIew.png" />
         </image>
      </ImageView>
      <BarChart fx:id="bcProductos" layoutX="14.0" layoutY="89.0" prefHeight="363.0" prefWidth="666.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" tickLabelFill="#ffffff" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" tickLabelFill="WHITE" />
        </yAxis>
      </BarChart>
      <Button fx:id="btnRegresar" alignment="CENTER" layoutX="475.0" layoutY="453.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="36.0" prefWidth="180.0" style="-fx-background-color: #F2133C; -fx-background-radius: 20;" text="Regresar" />
